{
   "build": {
      "cflags": "-fPIC -g",
      "ldflags": "-g",
      "platform": "posix",
      "toolchain": {
         "prefix": ""
      }
   },
   "debug": {
      "noisy_eeprom": "false",
      "logfile": "/tmp/radio-debug.log",
      "logtype": "file",
      "logmode": "0660",
      "loglevel": "debug",
      "mongoose": "false",
      "profiling": "true",
      "show_ts": "true",
      "targets": "*"
   },
   "features": {
      "alsa": "false",
      "auto_block_ptt": "false",
      "cat_kpa500": "true",
      "cat_yaesu": "true",
      "http": "true",
      "max_bands": 14,
      "mqtt": "true",
      "opus": "true",
      "pipewire": "true",
      "sqlite": "false"
   },
   "interface": {
      "cat": {
         "port": "pipe:/tmp/rustyradio.cat.socket",
         "enabled": "true"
      },
      "console": {
         "enabled": "true",
         "type": "stdio"
      }
   },
   "net": {
      "http": {
         "enabled": "true",
         "bind": "0.0.0.0",
         "port": 8420,
         "authdb": "./config/http.users",
         "authdb_dynamic": "true",
         "404_path": "./www/404.shtml",
         "tls_enabled": "true",
         "tls_cert": "/home/rigbox/ark/rustyrig-fw/config/cert.pem",
         "tls_port": 4420,
         "tls_key": "/home/rigbox/ark/rustyrig-fw/config/key.pem",
         "www_root": "./www"
      }
   },
   "eeprom": {
      "addr": "0x0000",
      "size": 1048576,
      "type": "mmap",
      "readonly": "false"
   },
   "owner": {
      "call": "N0CALL",
      "privs": "US/General"
   }
}
